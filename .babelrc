{
  "presets": [
    "es2015",
    "stage-2"
  ],
  "plugins": [
    [
      "transform-regenerator",
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": true,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ],
  "sourceMaps": true,
  "retainLines": true
}
